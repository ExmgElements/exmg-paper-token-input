    "url": "git://github.com/ExmgElements/exmg-paper-token-input.git"
  },
  "license": "MIT",
  "dependencies": {
    "@polymer/iron-flex-layout": "^3.0.1",
    "@polymer/iron-iconset-svg": "^3.0.1",
    "@polymer/iron-input": "^3.0.1",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-dropdown-menu": "^3.0.1",
    "@polymer/paper-icon-button": "^3.0.2",
    "@polymer/paper-input": "^3.0.1",
    "@polymer/paper-listbox": "^3.0.1",
    "@polymer/paper-menu-button": "^3.0.1",
    "@polymer/paper-styles": "^3.0.1",
    "@polymer/polymer": "^3.1.0",
    "lit-element": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@polymer/iron-component-page": "^4.0.1",
    "@polymer/iron-demo-helpers": "^3.0.2",
    "@polymer/paper-item": "^3.0.1",
    "@polymer/test-fixture": "^4.0.2",
    "@types/polymer": "^1.2.6",
    "@types/mocha": "^5.2.5",
    "@types/chai": "^4.1.7",
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "chai": "^4.2.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^5.2.0",
    "tslint": "^5.12.1",
    "tslint-config-exmg": "^1.0.2",
    "typedoc": "^0.14.2",
    "typescript": "^3.2.4",
    "wct-mocha": "^1.0.0",
    "web-animations-js": "^2.3.1"
  },
  "resolutions": {
    "type-detect": "1.0.0",
    "supports-color": "3.1.2",
    "inherits": "2.0.3",
    "samsam": "1.1.2"
  },
  "scripts": {
    "gen-docs": "typedoc --excludeNotExported --excludeExternals --excludePrivate --excludeProtected --ignoreCompilerErrors --exclude '{**/*test*,**/node_modules/**,**/test/**}' --out docs/ *.ts",
    "build": "npm run lint && npm run tsc",
    "tsc": "tsc -b ./tsconfig.json",
    "prepublishOnly": "npm run build",
    "preversion": "npm run build",
    "start": "gulp serve",
    "lint": "tslint -c ./tslint.json --project ./",
    "test-dev": "polymer test --npm -l chrome -p",
    "test": "polymer test --npm -l chrome --expanded",
    "test:integration": "polymer build # test that psk builds without error with the CLI"
  },
  "publishConfig": {
    "access": "public"
  }
}
