{
  "name": "@exmg/exmg-paper-token-input",
  "flat": true,
  "version": "3.0.3",
  "description": "Paper style token input element",
  "contributors": [
    "Ex Machina"
  ],
  "keywords": [
    "web-components",
    "polymer",
    "paper",
    "token",
    "input"
  ],
  "main": "exmg-paper-token-input.js",
  "files": [
    "exmg-*.js",
    "exmg-*.d.ts"
  ],
  "repository": {
